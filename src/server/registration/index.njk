{% extends 'layouts/page.njk' %}

{% block content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
      <div class="govuk-body" data-testid="app-page-body">
        <h2 class="govuk-heading-l">{{ localise('registration:packagingHeading') }}</h2>
        <p>
          <a
            href="{{ localiseUrl('/organisations/{{ organisationId }}/registrations/{{ registrationId }}/summary-logs/upload') }}"
            class="govuk-link"
          >
            {{ localise('registration:submitSummaryLog') }}
          </a>
        </p>
      </div>
    </div>
  </div>
{% endblock %}
